[Unit]
Description=Traktor Server

Wants=network.target
After=network.target

[Service]
ExecStart=/home/pi/projects/envs/traktor/bin/traktor gunicorn
WorkingDirectory=/home/pi/projects/traktor
User=pi
Group=pi
Type=simple
Restart=on-failure
PIDFile=/var/run/traktor.pid


[Install]
WantedBy=multi-user.target
